# === OpenAI / Gemini / LangSmith ===
# Required: OpenAI API key for LLM agents (GPT-4o-mini)
OPENAI_API_KEY=your_openai_api_key_here

# Optional: Gemini API key (provide dummy value if not used)
GEMENI_API_KEY=your_gemini_api_key_here

# Optional: LangSmith API key for tracing (provide dummy value if not used)
LANGSMITH_API_KEY=your_langsmith_api_key_here


# === Amadeus API ===
# Optional: Amadeus credentials for flight search and activity booking
# Provide dummy values if not using Amadeus
AMADEUS_API_SECRET=your_amadeus_api_secret_here
AMADEUS_API_KEY=your_amadeus_api_key_here
AMADEUS_TOKEN_URL=https://test.api.amadeus.com/v1/security/oauth2/token
AMADEUS_BASE_URL=https://test.api.amadeus.com


# === Auth0 ===
# Required: Auth0 configuration for API authentication
AUTH0_API_AUDIENCE=your_auth0_api_audience_here
AUTH0_DOMAIN=your_auth0_domain_here
AUTH0_ISSUER=your_auth0_issuer_here
AUTH0_ALGORITHMS=RS256
AUTH0_CLIENT_ID=your_auth0_client_id_here


# === Database ===
# Required: MongoDB connection for conversation persistence
MONGO_URI=mongodb://localhost:27017/vacation_agent

# Optional: Azure Cosmos DB configuration (if using Cosmos DB instead of MongoDB)
COSMOS_DB_ENDPOINT=your_cosmos_db_endpoint_here
COSMOS_DB_KEY=your_cosmos_db_key_here
COSMOS_DB_DATABASE_NAME=vacation_agent
COSMOS_DB_CONTAINER_NAME=conversations


# === Google Maps API ===
# Required: Google Maps API key for hotel and activity searches
GOOGLE_MAPS_API=your_google_maps_api_key_here
